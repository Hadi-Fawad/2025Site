<%- contentFor('style') %>
<style>
body {
    max-height: 100vh;
    display: flex;
    flex-direction: column;
    overflow-y: auto;
}

main {
    padding: 2rem;
}

h1 {
    font-size: 2rem;  /* Match main page size */
    margin-bottom: 1.5rem;
    margin-top: -2rem;  /* Add some controlled spacing above headers */

    color: var(--eink-black);
}

h2 {
    font-size: 2rem;  /* Match main page size */
    margin-bottom: 1.5rem;
    margin-top: 2rem;  /* Add some controlled spacing above headers */

    color: var(--eink-black);
}
.job-entry {
    margin-bottom: 2rem;
}

.job-header {
    display: flex;
    flex-direction: column;
    margin-bottom: 0.5rem;
}

.job-date {
    color: var(--eink-mid-dark);
    font-size: 1rem;
    margin-bottom: 0.5rem;
}

.job-title {
    font-size: 1rem;
}

.job-company a {
    color: var(--eink-darker);
    text-decoration: underline;
}

.job-details {
    list-style-type: none;
    padding-left: 1.5rem;  /* Increased from 0 */
}

.job-details li {
    margin-bottom: 0.5rem;
    padding-left: 1.5rem;  /* Increased from 1rem */
    position: relative;
}

.job-details li:before {
    content: "â€¢";
    position: absolute;
    left: 0;
}
</style>

